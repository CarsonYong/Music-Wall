



<div class ="songs">
<div class="row">
<% @songs.each do |song| %>

  <div class="col-md-6 text-center">
    <h3><a href="/musicwall/<%= song.id %>"><%= song.song_title %></a> <small>(by: <%= song.author %>)</small></h3>
    <p>
      <a href="http://<%= song.url %>"><%= song.url %></a>
    </p>
  </div>
<% end %>
</div>

</div>

